{
  "name": "mv-multipool-solana",
  "version": "0.0.1",
  "description": "mv-multipool-solana",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "anchor build",
    "test": "anchor test",
    "test1c": "anchor test-crunchyVsSmooth",
    "copyidl": "cp ./target/idl/abc.json ./app/src/idl.json",
    "testslv": "anchor test --skip-local-validator",
    "clear": "rm -rf .anchor target",
    "backup": "./backup1.sh",
    "log1": "less abc/.anchor/program-logs/*.abc.log",
    "network": "solana config get",
    "local": "solana config set --url localhost",
    "dev": "solana config set --url devnet",
    "addr": "solana address",
    "balance": "solana balance",
    "airdrop": "solana airdrop 1000",
    "validator": "solana-test-validator",
    "log": "solana logs",
    "deploylocal": "anchor deploy",
    "deploydev": "solana config set --url https://api.devnet.solana.com && anchor deploy --provider.cluster devnet",
    "deploymain": "solana config set --url https://api.mainnet-beta.solana.com && anchor deploy --provider.cluster mainnet",
    "build:program-rust": "cargo build-bpf --manifest-path=./src/program-rust/Cargo.toml --bpf-out-dir=dist/program",
    "pgid": "solana address -k target/deploy/abc-keypair.json",
    "pgid2": "solana address -k target/deploy/crunchy_vs_smooth-keypair.json",
    "cleancargo": "cargo clean --manifest-path=./src/program-rust/Cargo.toml && rm -rf ./dist"
  },
  "dependencies": {
    "@project-serum/anchor": "0.18.2",
    "@project-serum/common": "^0.0.1-beta.3",
    "@project-serum/serum": "^0.13.61",
    "@tsconfig/recommended": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/eslint": "^8.2.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "chalk": "^5.0.0",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-mocha": "9.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2"
  },
  "engines": {
    "node": ">=16"
  }
}
